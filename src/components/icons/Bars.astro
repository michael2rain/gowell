---
interface Props {
    id?: string;
    className?: string;
    width?: number;
    height?: number;
}

const { className, width = 24, height = 24 } = Astro.props;
---

<svg
    class={className}
    width={width}
    height={height}
    fill="none"
    xmlns="http://www.w3.org/2000/svg"
    ><path
        d="M20 7H4"
        stroke="#F8F8F8"
        stroke-width="1.5"
        stroke-linecap="round"></path><path
        opacity=".5"
        d="M20 12H4"
        stroke="#F8F8F8"
        stroke-width="1.5"
        stroke-linecap="round"></path><path
        d="M20 17H4"
        stroke="#F8F8F8"
        stroke-width="1.5"
        stroke-linecap="round"></path></svg
>
